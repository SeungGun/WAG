<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--시크바 배경-->
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">    <!--선 : 길이6dp -->
            <solid android:color="#FFFF00"/>
            <size android:width="115dp" android:height="20dp"/>
            <stroke android:width="1dp" android:color="#FFFFFF" />
            <gradient android:startColor="#FF0000" android:endColor="#00FF00"
                android:centerColor="#FFAF00"
                android:angle="180" android:type="linear"/>
        </shape>
    </item>

